{
  "name": "mail-link-extractor",
  "version": "0.1.0",
  "description": "App to extract link from mails and add them to pocket",
  "engineStrict": true,
  "engines": {
    "node": ">=8.9"
  },
  "main": "./.dist/src/index",
  "scripts": {
    "clean": "rm -rf .dist/",
    "lint:front": "eslint web/**/*.js --fix",
    "prebuild:front": "npm run lint:front",
    "build:front": "next build web/ && next export web/ -o .dist/static",
    "lint:back": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild:back": "npm run lint:back",
    "build:back": "tsc --project tsconfig.json --pretty",
    "format": "prettier --write '{api,web,worker}/**/*.{ts,js}'",
    "prebuild": "npm run clean && npm run format",
    "build": "npm run build:back && npm run build:front",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage && cat ./.coverage/lcov.info | codacy-coverage",
    "prestart": "npm run build",
    "start": "nf start api",
    "prerelease": "npm run build && npm run test",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@coolgk/base64": "2.0.4",
    "@heroku/foreman": "2.0.2",
    "cors": "2.8.4",
    "express": "4.16.3",
    "express-registry": "0.1.2",
    "googleapis": "33.0.0",
    "isomorphic-unfetch": "2.1.1",
    "localforage": "1.7.2",
    "mongoose": "5.2.13",
    "next": "6.1.2",
    "node-getpocket": "1.0.0",
    "pocket-promise": "1.1.0",
    "prop-types": "15.6.2",
    "ramda": "0.25.0",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "react-google-login": "3.2.1",
    "tslib": "1.9.3",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.2",
    "@types/localforage": "0.0.34",
    "@types/mongoose": "5.2.11",
    "@types/next": "6.1.4",
    "@types/node": "10.9.4",
    "@types/ramda": "0.25.37",
    "@types/react": "16.4.13",
    "@types/react-dom": "16.0.7",
    "@types/winston": "2.4.4",
    "babel-jest": "23.4.2",
    "codacy-coverage": "3.0.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "5.5.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.22.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "husky": "1.0.0-rc.13",
    "jest": "^23.5.0",
    "lint-staged": "7.2.2",
    "next-eslint": "0.0.4",
    "prettier": "1.14.2",
    "react-test-renderer": "16.5.0",
    "singleton-module-registry": "1.0.1",
    "standard-version": "4.4.0",
    "ts-jest": "^23.1.4",
    "tslint": "5.11.0",
    "typescript": "3.0.3"
  },
  "author": "Nicolas Pierre-Charles <pierrecharles.nicolas@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chipp972/mail-link-extractor.git"
  },
  "homepage": "https://github.com/chipp972/mail-link-extractor#readme",
  "bugs": {
    "url": "https://github.com/chipp972/mail-link-extractor/issues"
  },
  "license": "MIT"
}
