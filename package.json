{
  "name": "mail-link-extractor",
  "version": "0.1.0",
  "description": "App to extract link from mails and add them to pocket",
  "engineStrict": true,
  "engines": {
    "node": ">=8.9"
  },
  "main": "./dist/src/index",
  "typings": "./dist/src/index",
  "scripts": {
    "clean": "rm -rf lib",
    "format": "prettier --write '{src,test}/**/*.ts'",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "prebuild": "npm run clean && npm run format && npm run lint",
    "build": "tsc --project tsconfig.json --pretty",
    "build:watch": "tsc --watch --project tsconfig.json --pretty",
    "pretest": "npm run format && npm run lint && npm run build",
    "test": "mocha test/**/*.test.ts --exit",
    "test-only": "mocha test/**/*.test.ts --exit",
    "test:watch": "mocha --watch --reporter min",
    "prestart": "npm run build",
    "start": "nf start web",
    "prerelease": "npm run build",
    "release": "standard-version",
    "deploy": ""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "dependencies": {
    "@coolgk/base64": "2.0.4",
    "body-parser": "1.18.2",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "foreman": "2.0.0",
    "fs-extra": "5.0.0",
    "googleapis": "28.0.1",
    "node-getpocket": "1.0.0",
    "pocket-promise": "1.1.0",
    "prompts": "0.1.8",
    "ramda": "0.25.0",
    "tslib": "1.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.8",
    "@types/chai": "4.1.2",
    "@types/express": "4.11.1",
    "@types/express-session": "1.15.8",
    "@types/fs-extra": "5.0.1",
    "@types/mocha": "5.0.0",
    "@types/node": "9.6.1",
    "@types/ramda": "0.25.21",
    "@types/sinon": "4.3.0",
    "chai": "4.1.2",
    "husky": "0.14.3",
    "mocha": "5.0.5",
    "prettier": "1.11.1",
    "sinon": "4.5.0",
    "standard-version": "4.3.0",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.8.1"
  },
  "author": "Nicolas Pierre-Charles <pierrecharles.nicolas@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chipp972/mail-link-extractor.git"
  },
  "homepage": "https://github.com/chipp972/mail-link-extractor#readme",
  "bugs": {
    "url": "https://github.com/chipp972/mail-link-extractor/issues"
  },
  "license": "MIT"
}
